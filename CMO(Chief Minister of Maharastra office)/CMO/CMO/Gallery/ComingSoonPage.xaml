<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="CMO.ComingSoonPage"
             BackgroundColor="#09091a"
             Padding="10,10,10,10">
  <ContentPage.Resources>
    <ResourceDictionary>
      <Style x:Key="SubTitleStyle" TargetType="Label">
       <!-- <Setter Property="FontSize" Value="Small" />-->
        <Setter Property="FontAttributes" Value="Bold" />
        <Setter Property="TextColor" Value="White" />
      </Style>
    </ResourceDictionary>
  </ContentPage.Resources>
  <ContentPage.Content>
    <StackLayout BackgroundColor="#09091a" Padding="20,20,20,20">
      <!--<StackLayout BackgroundColor="White" Padding="0.5,0.5,0.5,0.5">-->
        <StackLayout BackgroundColor="#09091a">
          
            <Label x:Name="lbltext" Style="{StaticResource SubTitleStyle}" />
          
        </StackLayout>
      <!--</StackLayout>-->
    </StackLayout>
  </ContentPage.Content>
</ContentPage>